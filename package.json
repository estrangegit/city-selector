{
  "name": "city-selector",
  "version": "1.0.0",
  "description": "sandbox to practice reactJs framework",
  "main": "index.tsx",
  "author": "Etienne Estrangin",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "https://github.com/estrangegit/city-selector.git"
  },
  "dependencies": {
    "actions": "latest",
    "axios": "latest",
    "bootstrap": "latest",
    "jquery": "latest",
    "popper.js": "latest",
    "react": "latest",
    "react-bootstrap": "latest",
    "react-dom": "latest",
    "react-redux": "latest",
    "reducers": "latest",
    "redux": "latest",
    "redux-saga": "latest",
    "saga": "latest",
    "sass": "^1.17.4"
  },
  "devDependencies": {
    "@types/bootstrap": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@types/react-redux": "latest",
    "concurrently": "latest",
    "node-sass": "latest",
    "parcel-bundler": "latest",
    "prettier": "latest",
    "redux-devtools-extension": "latest",
    "tslint": "latest",
    "tslint-config-airbnb": "latest",
    "tslint-config-prettier": "latest",
    "tslint-react": "latest",
    "typescript": "latest"
  },
  "scripts": {
    "develop": "concurrently -r \"yarn:type-check:watch\" \"yarn:start\"",
    "start": "parcel ./src/index.html -p 3500",
    "build": "parcel build ./src/index.html --public-url \"./\"",
    "build-check": "yarn run type-check && parcel build ./src/index.html --public-url \"./\"",
    "prettify": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch"
  },
  "alias": {
    "actions": "./src/actions",
    "components": "./src/components",
    "containers": "./src/containers",
    "model": "./src/model",
    "reducers": "./src/reducers",
    "sass": "./src/sass"
  }
}
